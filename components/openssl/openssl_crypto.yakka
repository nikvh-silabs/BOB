name: OpenSSL Crypto

includes:
  local:
    - include
    - providers/common/include
    - providers/implementations/include
    - crypto/evp
    - .

defines:
  local:
    - AES_ASM
    - BSAES_ASM
    - CMLL_ASM
    - ECP_NISTZ256_ASM
    - GHASH_ASM
    - KECCAK1600_ASM
    - MD5_ASM
    - OPENSSL_BN_ASM_GF2m
    - OPENSSL_BN_ASM_MONT
    - OPENSSL_BN_ASM_MONT5
    - OPENSSL_CPUID_OBJ
    - OPENSSL_IA32_SSE2
    - PADLOCK_ASM
    - POLY1305_ASM
    - RC4_ASM
    - SHA1_ASM
    - SHA256_ASM
    - SHA512_ASM
    - VPAES_ASM
    - WHIRLPOOL_ASM
    - X25519_ASM
    - OPENSSLDIR=\".\"
    - ENGINESDIR=\"engines-3\"
    - MODULESDIR=\"ossl-modules\"
    - "OPENSSL_PIC"
    - "OPENSSL_BUILDING_OPENSSL"
    - OPENSSL_NO_DEPRECATED_3_0
    - "L_ENDIAN"
    - "UNICODE"
    - "_UNICODE"
    - "NDEBUG"

supports:
  components:
    msvc:
      defines:
        local:
          - WIN32_LEAN_AND_MEAN
          - "OPENSSL_SYS_WIN32"
          - "_CRT_SECURE_NO_DEPRECATE"
          - "_WINSOCK_DEPRECATED_NO_WARNINGS"
      flags:
        c:
          local:
            - /Zi
            - /MT /Zl /Gs0 /GF /Gy /W3 /wd4090 /nologo
            - /O2
            - /Fdossl_static.pdb

